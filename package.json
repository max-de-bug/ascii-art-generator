{
  "name": "ascii-art-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:backend": "cd app/backend/ascii-art-generator-backend && npm run start:dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix-colors \"cyan,magenta\"",
    "build": "npm run build:wasm && next build --turbopack",
    "build:wasm": "node scripts/build-wasm.js",
    "build:wasm:dev": "cd wasm-ascii && wasm-pack build --target web --out-dir pkg --dev",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.9.11",
    "@coral-xyz/anchor": "^0.32.1",
    "@lighthouse-web3/sdk": "^0.4.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/typeorm": "^11.0.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@solana/spl-token": "^0.4.8",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/web3.js": "^1.95.8",
    "@tanstack/react-query": "^5.90.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "mysql2": "^3.15.3",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "nft.storage": "^7.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reflect-metadata": "^0.2.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "typeorm": "^0.3.27",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vercel/node": "^5.5.13",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
