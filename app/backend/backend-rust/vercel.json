{
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-rust@5.0.0",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.rs"
    },
    {
      "src": "/api/nft/user/([^/]+)/level",
      "dest": "/api/user_level.rs?wallet=$1"
    },
    {
      "src": "/api/nft/user/([^/]+)/shard-status",
      "dest": "/api/user_shard_status.rs?wallet=$1"
    },
    {
      "src": "/api/nft/user/([^/]+)",
      "dest": "/api/user_nfts.rs?wallet=$1"
    },
    {
      "src": "/api/nft/mint/([^/]+)",
      "dest": "/api/nft_by_mint.rs?mint=$1"
    },
    {
      "src": "/api/nft/statistics",
      "dest": "/api/statistics.rs"
    },
    {
      "src": "/api/nft/indexer/status",
      "dest": "/api/indexer_status.rs"
    },
    {
      "src": "/api/nft/buybacks/statistics",
      "dest": "/api/statistics.rs"
    },
    {
      "src": "/api/nft/buybacks",
      "dest": "/api/buybacks.rs"
    },
    {
      "src": "/health",
      "dest": "/api/health.rs"
    },
    {
      "src": "/nft/user/([^/]+)/level",
      "dest": "/api/user_level.rs?wallet=$1"
    },
    {
      "src": "/nft/user/([^/]+)/shard-status",
      "dest": "/api/user_shard_status.rs?wallet=$1"
    },
    {
      "src": "/nft/user/([^/]+)",
      "dest": "/api/user_nfts.rs?wallet=$1"
    },
    {
      "src": "/nft/mint/([^/]+)",
      "dest": "/api/nft_by_mint.rs?mint=$1"
    },
    {
      "src": "/nft/statistics",
      "dest": "/api/statistics.rs"
    },
    {
      "src": "/nft/indexer/status",
      "dest": "/api/indexer_status.rs"
    },
    {
      "src": "/nft/buybacks/statistics",
      "dest": "/api/statistics.rs"
    },
    {
      "src": "/nft/buybacks",
      "dest": "/api/buybacks.rs"
    }
  ],
  "env": {
    "RUST_BACKTRACE": "1"
  },
  "build": {
    "env": {
      "RUST_BACKTRACE": "1"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ]
}

